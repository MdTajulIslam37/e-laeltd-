body{
	/* background-color:#8bd3c9; */
	font-family:arial;
}
.headerr{
	height:300px;
	width:100%;
	/* background-image:url("bg.jpg"); */
	background-color:pink;
}
.menu{
	width:100%;
	height:100px;
	padding:10px;

}
.menu ul{
	list-style:none;
	float:right;
	margin:0;
	padding:0px, 0px, 0px, 20px;
	display:flex;
}
.menu ul img{
	margin-top:-25px;
}
.menu ul li a{
	line-height:80px;
	color:black;
	padding:20px;
	text-decoration:none;
	font-size:20px;
	font-weight:blod;
	text-transform:uppercase;
}
.menu ul li a:hover{
	background:#8bd3c9;
	border-radius:12px;
}

.phones_div{
	height:250px;
	width:100%;
	background-color:#fff;
	border-radius:10px;
}
